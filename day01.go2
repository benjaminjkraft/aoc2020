package main

import "fmt"

func day01a(input []int) (string, error) {
	for _, i := range input {
		for _, j := range input {
			if i+j == 2020 {
				return fmt.Sprint(i * j), nil
			}
		}
	}
	return "", fmt.Errorf("no solution found")
}

func day01b(input []int) (string, error) {
	for _, i := range input {
		for _, j := range input {
			for _, k := range input {
				if i+j+k == 2020 {
					return fmt.Sprint(i * j * k), nil
				}
			}
		}
	}
	return "", fmt.Errorf("no solution found")
}
